@using CarVillaProject.BL.ViewModels

@model CarCreateVM

<form asp-area="Admin" asp-controller="Car" asp-action="Create" metod="post" enctype="multipart/form-data">
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Model</label>
        <input asp-for="Model" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Brand</label>
        <input asp-for="Brand" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>
   
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Automatic</label>
        <input asp-for="SpeedBoxCategory" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Price</label>
        <input asp-for="Price" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Descriptions</label>
        <input asp-for="Description" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">ImgFile</label>
        <input asp-for="ImgFile" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>